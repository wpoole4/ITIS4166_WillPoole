    <body>
        <%- include('../components/header.ejs')%>
        <main>
            <form action = '/event/events' method = 'POST' class="new-form">
                <label for = "category">Category </label>
                <select id = "category" name = "category">
                    <option value="" disabled selected>-- Select a Category -- </option>
                    <option value = "Class">Class</option>
                    <option value = "Q&A">Q&A</option>
                    <option value = "Workout">Workout</option>
                    <option value = "Workshop">Workshop</option>
                    <option value = "Other">Other</option>
                </select> <br>
                <label for = "title">Title </label>
                <input type = "text" id = "title" name = "title" placeholder = "Insert Title Here" required size = "57"> <br>
                <label for = "details">Description </label>
                <input type = "text" id = "details" name = "details" placeholder = "Insert Description Here" required size = "57"> <br>
                <label for = "location">Location </label>
                <input type = "text" id = "location" name = "location" placeholder = "Insert Location" required size = "57"> <br>
                <label for="hostname">Host Name </label>
                <input type = "text" id = "hostname" name = "hostname" placeholder = "Insert Host(s)" required size = "57"> <br>
                <label for="startdate">Start </label>
                <input type="datetime-local" id="startdate" name="startdate" required> <br>
                <label for="enddate"> End </label>
                <input type="datetime-local" id="enddate" name="enddate" required> <br>
                <label for="image">Image </label>
                <input type="file" id="image" name="image" accept="image/*"> <br>
                <input type = "submit" id = "submit" value="Submit">
            </form>
        </main>
        <%- include('../components/footer.ejs')%>
    </body>
</html>